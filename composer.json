{
  "name": "faizalmansor/lumen-6-maintenance-mode",
  "type": "library",
  "description": "Define application maintenance mode.",
  "keywords": ["lumen", "maintenance", "mode"],
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "laravel/lumen-framework": "^6.0"
  },
  "authors": [
    {
      "name": "Faizal Mansor",
      "email": "faizalmansor@gmail.com"
    }
  ],
  "require-dev": {
    "laravel/lumen":"~6.0",
    "phpunit/phpunit": "~7.0",
    "fzaninotto/faker": "^1.4.0",
    "friendsofphp/php-cs-fixer": "^2.15"
  },
  "autoload": {
    "psr-4": {
      "faizalmansor\\MaintenanceMode\\": "src"
    }
  }
  ,
  "autoload-dev": {
    "psr-4": {
      "faizalmansor\\MaintenanceMode\\Testing\\": "src/tests/MaintenanceMode/"
    }
  },
  "config":{
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
