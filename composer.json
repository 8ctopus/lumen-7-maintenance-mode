{
  "name": "j3rrey/lumen-maintenance-mode",
  "type": "library",
  "description": "Define application maintenance mode.",
  "keywords": ["lumen", "maintenance", "mode"],
  "license": "MIT",
  "authors": [
    {
      "name": "no one",
      "email": "nono@nono.com"
    },
  ],
  "require": {
    "php": ">=7.0",
    "laravel/lumen-framework": "~5.8"
  },
  "require-dev": {
    "laravel/lumen":"~5.8",
    "phpunit/phpunit": "~7.0",
    "fzaninotto/faker": "^1.4.0",
    "friendsofphp/php-cs-fixer": "^2.15",
  },
  "autoload": {
    "psr-4": {
      "J3rrey\\MaintenanceMode\\": "src"
    }
  }
  ,
  "autoload-dev": {
    "psr-4": {
      "J3rrey\\MaintenanceMode\\Testing\\": "src/tests/MaintenanceMode/"
    }
  },
  "config":{
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
